<h1>List of Users signed up</h1>
<div class="row">
  <div class="col-md-8">
    <table class="table table-striped">
      <% @users.each do |user| %>
         <tr>
           <td><%= user.first_name%></td>
            <td><%= user.last_name%></td>
            <td><%= link_to user.email, user_path(user) %></td>
          </tr>
        <% end %>
    </table>
  </div>
</div>